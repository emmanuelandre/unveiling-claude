    "marked": "^15.0.4",
    "marked-terminal": "^7.2.1",
    "openai": "^4.73.1",
    "ora": "^8.1.1",
    "simple-git": "^3.27.0",
    "tiktoken": "^1.0.17",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
